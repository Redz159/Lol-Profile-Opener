{
  "manifest_version": 3,
  "name": "LoL Profile Opener",
  "version": "1.1",
  "description": "Opens lol profiles on different sites for you :)",
  "permissions": ["tabs", "storage"],
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png"
    }
  }
}
